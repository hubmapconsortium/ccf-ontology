        <rdfs:label>2P-Seq</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ebi.ac.uk/efo/EFO_0008640 -->

    <owl:Class rdf:about="http://www.ebi.ac.uk/efo/EFO_0008640">
        <rdfs:subClassOf rdf:resource="http://www.ebi.ac.uk/efo/EFO_0001457"/>
        <metadata:prefixIRI>efo:EFO_0008640</metadata:prefixIRI>
        <obo:IAO_0000115>Strand-specific 3’-end RNA-seq</obo:IAO_0000115>
